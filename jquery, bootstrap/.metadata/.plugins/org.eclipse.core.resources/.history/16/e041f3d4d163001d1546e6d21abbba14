	<!-- > : 하위객체를 의미 -->
	<!-- + : 동등객체를 의미 -->
	<!-- () : 그룹 -->
	<!-- [] : 대괄호는 속성 -->
	<!-- . : class속성 -->
	<!-- # : id 속성 -->
	<!-- h1{중괄호는 속성} tab키 치면 아래같이 만들어짐. -->
	<h1>중괄호는 속성</h1>
	<!-- h1+p tab키 치면 아래  -->
	<h1></h1>
	<p></p>
	<!-- h1>p tab-->
	<h1>
		<p></p>
	</h1>
	<!-- (h1>p)*5 는 h1>p를 5개 만들으라는 뜻 -->
	<h1>
		<p></p>
	</h1>
	<h1>
		<p></p>
	</h1>
